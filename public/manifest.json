{
    "name": "CPF Tool",
    "description": "Tool to help developers and QA to speed up valid CPF generation",
    "version": "0.2",
    "manifest_version": 3,
    "background": {
        "service_worker": "js/background.js"
    },
    "permissions": [
        "contextMenus",
        "activeTab",
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "js/content_script.js"
            ],
            "run_at": "document_end",
            "all_frames": false
        }
    ]
}